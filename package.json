    "license": "ISC",
    "devDependencies": {
        "@typechain/ethers-v6": "^0.4.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/node": "^18.15.11",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "eslint": "^8.37.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^2.0.22",
        "onchange": "^7.1.0",
        "prettier": "^2.8.7",
        "rimraf": "^4.4.1",
        "ts-node": "^10.9.1",
        "typechain": "^8.2.0",
        "typescript": "^5.0.3"
    },
    "dependencies": {
        "async-mutex": "^0.4.0",
        "axios": "^1.3.6",
