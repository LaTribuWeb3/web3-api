    "version": "1.0.0",
    "main": "Api.ts",
    "scripts": {
        "start": "npx nodemon",
        "build": "rimraf ./build && tsc",
        "lint": "eslint . --ext .ts",
        "lintfix": "eslint . --ext .ts --fix",
        "prettier-format": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
